{
  "name": "banigram",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-scripts": "3.0.1"
  },
  "scripts": {
    "start": "react-scripts-with-stylus start src/index.styl",
    "build": "react-scripts-with-stylus build src/index.styl",
    "lint": "./node_modules/.bin/eslint src/",
    "storybook": "run-p stylus:components storybook:launch",
    "storybook:launch": "start-storybook -p 9001",
    "stylus:components": "stylus -w src/components/index.styl -o src/components/index.css",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@agillic/eslint-config": "0.0.4",
    "@babel/core": "^7.4.4",
    "@storybook/addon-actions": "^5.0.11",
    "@storybook/react": "^5.0.11",
    "create-react-app-stylus": "^1.1.1",
    "npm-run-all": "^4.1.5",
    "stylus": "^0.54.5"
  }
}
